import java.util.*;
class FactorialCalculator 
{
    public static void main(String args[]) 
    {
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the value of n: ");
        int n = s.nextInt();
        long factorial = 1;
        for (int i = 1; i <= n; i++) 
        {
            factorial *= i;
        }
        s.close();
        System.out.println("The factorial of " + n + " is: " + factorial);
    }
}
